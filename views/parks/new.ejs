<% layout('layouts/boilerplate') %>
<h1>Add new park</h1>
<form method="POST" action="/parks">
  <label for="title">Title</label>
  <input type="text" id="title" name="park[title]" />
  <label for="location">Location</label>
  <input type="text" id="location" name="park[location]" />
  <button>Add</button>
</form>
